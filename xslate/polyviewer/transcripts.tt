	: if $mane == 1 {

		
		<td>
			: if $item.alphamissense == -99 {
   	 			<button "type="button" class= "btn btn-xs btn-primary " style="border-color:#D0D0D0;background-color:white;color:black;font-size: 8px;font-family:  Verdana"><span class='glyphicon glyphicon-minus' aria-hidden='true'/></button>
			: }
			: else if $item.alphamissense > 0.564 { 
				<button type="button" class= "btn btn-xs btn-primary " style="border-color:#e74c3c;background-color:white;color:#e74c3c;font-size: 8px;font-family:  Verdana">likely pathogenic</button>
			: }
			: else if $item.alphamissense < 0.34 {
				<button "type="button" class= "btn btn-xs btn-primary " style="border-color:#188c52;background-color:white;color:green;font-size: 8px;font-family:  Verdana"> likely benign </button>
			: }
			: else  {
				<button type="button" class= "btn btn-xs btn-primary " style="border-color:blue;background-color:white;color:blue;font-size: 8px;font-family:  Verdana">ambigous</button>
			: }
		</td>

	:}
		
	<td>
		<span class="badge badge-success badge-xs" style="border-color:4CAF50;background-color:#FFFFFF;color:
		: if $item.dbscsnv == "-"  {

   	 		black;font-size :8px;"><span class='glyphicon glyphicon-minus' aria-hidden='true'/></span> 
		: }
		: else if $item.dbscsnv ==  "-:-" {
			black;font-size :8px;"><span class='glyphicon glyphicon-minus' aria-hidden='true'/></span> 
		: }
		: else if $item.dbscsnv == -99 {
   	 		black;font-size :8px;"><span class='glyphicon glyphicon-minus' aria-hidden='true'/></span> 
		: }
	 	: else if $item.dbscsnv >= 0.6 {
   		 	red;font-size :8px;"><: $item.dbscsnv :></span> 
		: }
		: else  {
			#188c52;font-size :8px;"><: $item.dbscsnv :></span>
		: }
	</td>
		
	<td>
		<table>
		<td>
		
		: if $item.spliceAI == -99 {

			<button onClick="alert('<: $text_alert :>')" "type="button" class= "btn btn-xs btn-primary " style="border-color:#D0D0D0;background-color:white;color:black;font-size: 8px;font-family:  Verdana"><span class='glyphicon glyphicon-minus' aria-hidden='true'/></button>
		:}
		: else if $item.spliceAI >=  0.8 {
   	 		<button onClick="alert('<: $text_alert :>')" "type="button" class= "btn btn-xs btn-primary " style="border-color:#e74c3c;background-color:white;color:#e74c3c;font-size: 8px;font-family:  Verdana"><: $item.spliceAI_cat :>:<: $item.spliceAI :></button>
		: }
		: else if $item.spliceAI >= 0.5 {
			<button onClick="alert('<: $text_alert :>')" "type="button" class= "btn btn-xs btn-primary " style="border-color:#ff9800;background-color:white;color:#ff9800;font-size: 8px;font-family:  Verdana;border-color:white"><: $item.spliceAI_cat :>:<: $item.spliceAI :></button>
		: }
		: else if   $item.spliceAI >= 0 {
			<button onClick="alert('<: $text_alert :>')" "type="button" class= "btn btn-xs btn-primary " style="border-color::blue;background-color:white;color:blue;font-size: 8px;font-family:  Verdana;border-color:white"><: $item.spliceAI_cat :><: $item.spliceAI :></button>
		: }
		: else{
			
		: }
	 
		</td>
		<td style='padding-left:3px;'>
			<button onClick="window.open('https://spliceailookup.broadinstitute.org/#variant=<: $var_id :>&hg=37&distance=1000&mask=0&precomputed=0','_blank')" "type="button" class= "btn btn-xs btn-primary " style="border-color:#D0D0D0;background-color: white;font-size: 9px;font-family: Verdana;color:black"><span class='glyphicon glyphicon-pencil' aria-hidden='true'/></button>
		</td>
		</table>
	</td>
