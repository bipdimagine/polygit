		: }
		: else  if $item.polyphen >= 0.908 {
   	 		<button "type="button" class= "btn btn-xs btn-primary " style="border-color:#e74c3c;background-color:white;color:#e74c3c;font-size: 8px;font-family:  Verdana"><: $item.polyphen :></button>
		: }
