 <th style="text-align: center;">nm</th>
<th style="text-align: center;">confidence</th>
 <th style="text-align: center;">exon</th>
 <th style="text-align: center;">nomenclature</th>
 <th style="text-align: center;">codons</th> 
<th style="text-align: center;">codons_AA</th> 
<th style="text-align: center;">polyphen</th> 
<th style="text-align: center;">sift</th> 
<th style="text-align: center;">cadd</th> 
<th style="text-align: center;">revel</th>
: if $mane == 1 {
